<h1>Listing relation_services</h1>

<table>
  <thead>
    <tr>
      <th>Servicio</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relation_services.each do |relation_service| %>
      <tr>
        <td><%= relation_service.servicio %></td>
        <td><%= relation_service.item %></td>
        <td><%= link_to 'Show', relation_service %></td>
        <td><%= link_to 'Edit', edit_relation_service_path(relation_service) %></td>
        <td><%= link_to 'Destroy', relation_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relation service', new_relation_service_path %>
